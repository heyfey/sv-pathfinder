{
  "name": "sv-pathfinder",
  "publisher": "heyfey",
  "displayName": "sv-pathfinder",
  "description": "There are venoms and virtues aplenty in the wilds, if you know where to look.",
  "icon": "media/icon.png",
  "version": "0.0.1",
  "repository": "https://github.com/heyfey/sv-pathfinder",
  "engines": {
    "vscode": "^1.97.0"
  },
  "categories": [
    "Debuggers",
    "Programming Languages",
    "Other"
  ],
  "keywords": [
    "SystemVerilog",
    "Verilog",
    "RTL",
    "GLS",
    "Testbench",
    "Debuggers",
    "Waveform",
    "Logic Simulation",
    "Logic Analyzer"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "configuration": {
      "title": "SV-Pathfinder",
      "properties": {
        "sv-pathfinder.remotePathPrefix": {
          "type": "string",
          "scope": "resource",
          "description": "Replace the remote path prefix with the local path prefix when jumping to source files. This is useful when the debug database is generated on the different machine. Only take effect when sv-pathfinder.localPathPrefix is also set."
        },
        "sv-pathfinder.localPathPrefix": {
          "type": "string",
          "scope": "resource",
          "description": "Replace the remote path prefix with the local path prefix when jumping to source files. This is useful when the debug database is generated on the different machine. Only take effect when sv-pathfinder.remotePathPrefix is also set."
        }
      }
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "svPathfinderActivityBar",
          "title": "SV-Pathfinder",
          "icon": "media/activityBar.svg"
        }
      ]
    },
    "viewsWelcome": [
      {
        "view": "openedDesignsView",
        "contents": "Open a design database which can be debugged (uhdm, elab.kz or daidar) \n[Open Design](command:sv-pathfinder.openDesign) \n [learn more](https://github.com/heyfey/sv-pathfinder)."
      }
    ],
    "views": {
      "svPathfinderActivityBar": [
        {
          "id": "openedDesignsView",
          "name": "Designs"
        },
        {
          "id": "hierarchyView",
          "name": "Hierarchy"
        },
        {
          "id": "driversView",
          "name": "Drivers",
          "when": "sv-pathfinder.driversViewVisible"
        },
        {
          "id": "loadsView",
          "name": "Loads",
          "when": "sv-pathfinder.loadsViewVisible"
        },
        {
          "id": "moduleInstancesView",
          "name": "Instances",
          "when": "sv-pathfinder.moduleInstancesViewVisible"
        }
      ]
    },
    "menus": {
      "editor/title/context": [],
      "view/title": [
        {
          "command": "sv-pathfinder.openDesign",
          "when": "view == openedDesignsView",
          "group": "navigation"
        },
        {
          "command": "sv-pathfinder.goBack",
          "when": "view == hierarchyView",
          "group": "navigation"
        },
        {
          "command": "sv-pathfinder.goForward",
          "when": "view == hierarchyView",
          "group": "navigation"
        }
      ],
      "view/item/context": [
        {
          "command": "sv-pathfinder.openWaveform",
          "when": "view == 'openedDesignsView' && viewItem == 'designItem'",
          "group": "inline@0"
        },
        {
          "command": "sv-pathfinder.closeDesign",
          "when": "view == 'openedDesignsView' && viewItem == 'designItem'",
          "group": "inline@1"
        },
        {
          "command": "sv-pathfinder.closeWaveform",
          "when": "view == 'openedDesignsView' && viewItem == 'waveformItem'",
          "group": "inline"
        },
        {
          "command": "sv-pathfinder.openWaveform",
          "when": "view == 'openedDesignsView' && viewItem == 'designItem'",
          "group": "0_default@1"
        },
        {
          "command": "sv-pathfinder.closeWaveform",
          "when": "view == 'openedDesignsView' && viewItem == 'waveformItem'",
          "group": "0_default@4"
        },
        {
          "command": "sv-pathfinder.revealWaveform",
          "when": "view == 'openedDesignsView' && viewItem == 'waveformItem'",
          "group": "0_default@3"
        },
        {
          "command": "sv-pathfinder.gotoDefinition",
          "when": "view == 'hierarchyView'",
          "group": "0_default@3"
        },
        {
          "command": "sv-pathfinder.gotoInstantiation",
          "when": "view == 'hierarchyView' && viewItem == 'scopeItem'",
          "group": "0_default@4"
        },
        {
          "command": "sv-pathfinder.copyName",
          "when": "view == 'hierarchyView'",
          "group": "0_default@5"
        },
        {
          "command": "sv-pathfinder.addToWaveform",
          "when": "view == 'hierarchyView' && viewItem == 'varItem'",
          "group": "0_default@6"
        },
        {
          "command": "sv-pathfinder.addAllInScopeToWaveform",
          "when": "view == 'hierarchyView' && viewItem == 'scopeItem'",
          "group": "0_default@7"
        }
      ],
      "editor/context": [
        {
          "command": "sv-pathfinder.selectInstance",
          "when": "resourceLangId == verilog && sv-pathfinder.moduleInstancesViewVisible",
          "group": "1_designNavigation@0"
        },
        {
          "command": "sv-pathfinder.traceDriver",
          "when": "resourceLangId == verilog && sv-pathfinder.driversViewVisible",
          "group": "1_designNavigation@1"
        },
        {
          "command": "sv-pathfinder.traceLoad",
          "when": "resourceLangId == verilog && sv-pathfinder.loadsViewVisible",
          "group": "1_designNavigation@2"
        },
        {
          "command": "sv-pathfinder.showInHierarchyView",
          "when": "resourceLangId == verilog",
          "group": "1_designNavigation@3"
        },
        {
          "command": "sv-pathfinder.copyHierarchyName",
          "when": "resourceLangId == verilog",
          "group": "1_designNavigation@4"
        },
        {
          "command": "sv-pathfinder.addToWaveformViewer",
          "when": "resourceLangId == verilog",
          "group": "1_designNavigation@5"
        }
      ]
    },
    "commands": [
      {
        "command": "sv-pathfinder.openDesign",
        "title": "Open Design",
        "icon": "$(file-directory-create)"
      },
      {
        "command": "sv-pathfinder.closeDesign",
        "title": "Close Design",
        "icon": "$(close)"
      },
      {
        "command": "sv-pathfinder.openWaveform",
        "title": "Open Waveform",
        "icon": "$(add)"
      },
      {
        "command": "sv-pathfinder.closeWaveform",
        "title": "Close Waveform",
        "icon": "$(close)"
      },
      {
        "command": "sv-pathfinder.revealWaveform",
        "title": "Open Waveform"
      },
      {
        "command": "sv-pathfinder.selectDesign",
        "title": "Select Design"
      },
      {
        "command": "sv-pathfinder.gotoDefinition",
        "title": "Go to Definition"
      },
      {
        "command": "sv-pathfinder.gotoInstantiation",
        "title": "Go to Instantiation"
      },
      {
        "command": "sv-pathfinder.copyName",
        "title": "Copy Name"
      },
      {
        "command": "sv-pathfinder.addToWaveform",
        "title": "Add to Waveform Viewer"
      },
      {
        "command": "sv-pathfinder.addAllInScopeToWaveform",
        "title": "Add All Variables in Scope to Waveform Viewer"
      },
      {
        "command": "sv-pathfinder.selectInstance",
        "title": "Select Instance"
      },
      {
        "command": "sv-pathfinder.traceDriver",
        "title": "Trace Driver",
        "enablement": "sv-pathfinder.isCommandEnabled"
      },
      {
        "command": "sv-pathfinder.traceLoad",
        "title": "Trace Load",
        "enablement": "sv-pathfinder.isCommandEnabled"
      },
      {
        "command": "sv-pathfinder.showInHierarchyView",
        "title": "Show in Hierarchy View",
        "enablement": "sv-pathfinder.isCommandEnabled"
      },
      {
        "command": "sv-pathfinder.addToWaveformViewer",
        "title": "Add to Waveform Viewer",
        "enablement": "sv-pathfinder.isCommandEnabled"
      },
      {
        "command": "sv-pathfinder.copyHierarchyName",
        "title": "Copy Hierarchy Name",
        "enablement": "sv-pathfinder.isCommandEnabled"
      },
      {
        "command": "sv-pathfinder.goBack",
        "title": "Go Back",
        "icon": "$(arrow-left)",
        "enablement": "sv-pathfinder.isGoBackEnabled"
      },
      {
        "command": "sv-pathfinder.goForward",
        "title": "Go Forward",
        "icon": "$(arrow-right)",
        "enablement": "sv-pathfinder.isGoForwardEnabled"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "npm run check-types && npm run lint && node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "npm run check-types && npm run lint && node esbuild.js --production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/emscripten": "^1.40.1",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.97.0",
    "@typescript-eslint/eslint-plugin": "^8.22.0",
    "@typescript-eslint/parser": "^8.22.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "esbuild": "^0.24.2",
    "eslint": "^9.19.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "kuzu": "^0.9.0",
    "web-tree-sitter": "^0.25.6"
  }
}
